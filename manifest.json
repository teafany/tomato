{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0",
    "description": "Pomodoro timer with a to-do list.",
    "action": {
      "default_icon": "images/tomato.png"
    },
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [{
      "resources": ["video.html", "videos/gameplay.mp4"],
      "matches": ["<all_urls>"]
    }]
  }
  